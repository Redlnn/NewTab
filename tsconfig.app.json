{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "paths": {
    "#i18n": ["./.wxt/i18n/index.ts"],
    "#i18n/*": ["./.wxt/i18n/index.ts/*"]
  },
  "include": ["./entrypoints/**/*", "./.wxt/i18n/**/*"],
  "exclude": ["./.output", "./.wxt/types/imports.d.ts"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue"
  }
}
